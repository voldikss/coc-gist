{
  "name": "coc-gist",
  "version": "0.0.2",
  "description": "gist extension for coc.nvim",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/voldikss/coc-gist"
  },
  "publisher": "voldikss",
  "keywords": [
    "coc.nvim",
    "gist"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {}
    },
    "commands": [
      {
        "title": "create a new gist from current file",
        "command": "gist.create"
      }
    ]
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@voldikss/tsconfig": "^1.0.0",
    "coc.nvim": "^0.0.77",
    "colors": "^1.4.0",
    "eslint": "^7.7.0",
    "request-light": "^0.3.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "fetch-gists": "^3.0.0"
  }
}
