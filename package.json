{
  "name": "coc-gist",
  "version": "0.1.0",
  "description": "gist extension for coc.nvim",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/voldikss/coc-gist"
  },
  "publisher": "voldikss",
  "keywords": [
    "coc.nvim",
    "gist"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {}
    },
    "commands": [
      {
        "title": "create a new gist from current file",
        "command": "gist.create"
      }
    ]
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@octokit/rest": "^18.0.12",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@voldikss/tsconfig": "^1.0.0",
    "coc.nvim": "^0.0.80",
    "colors": "^1.4.0",
    "eslint": "^7.17.0",
    "express": "^4.17.1",
    "fetch-gists": "^4.0.0",
    "node-fetch": "^2.6.1",
    "request-light": "^0.4.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  }
}
